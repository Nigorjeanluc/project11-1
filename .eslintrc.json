{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "consistent-return": 2,
        "indent": 0,
        "no-else-return": 1,
        "semi": [1, "always"],
        "space-unary-ops": 2,
        "linebreak-style": [0, "never"],
        "global-require": 0,
        "no-console": 0,
        "eslint linebreak-style": [0, "error", "windows"],
        "eol-last": 0
    }
}